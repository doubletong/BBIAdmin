      script(src='js/vendor/jquery-3.4.1.min.js')
      script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js')
      script(src='js/bootstrap/bootstrap.min.js')
      script(src='js/app.js')
